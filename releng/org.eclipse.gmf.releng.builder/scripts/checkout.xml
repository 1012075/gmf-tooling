<?xml version="1.0"?>
<project default="all">
	<property file="build.properties" />
	
	<target name="plugins">
		<cvs cvsRoot=":pserver:anonymous@dev.eclipse.org:/cvsroot/technology" package="org.eclipse.gmf/plugins" dest="${checkoutDirectory}" />
	</target>
	<target name="features">
		<cvs cvsRoot=":pserver:anonymous@dev.eclipse.org:/cvsroot/technology" package="org.eclipse.gmf/features" dest="${checkoutDirectory}" />
	</target>
	<target name="tests">
		<cvs cvsRoot=":pserver:anonymous@dev.eclipse.org:/cvsroot/technology" package="org.eclipse.gmf/tests" dest="${checkoutDirectory}" />
	</target>
	<target name="examples">
		<cvs cvsRoot=":pserver:anonymous@dev.eclipse.org:/cvsroot/technology" package="org.eclipse.gmf/examples" dest="${checkoutDirectory}" />
	</target>
	<target name="docs">
		<cvs cvsRoot=":pserver:anonymous@dev.eclipse.org:/cvsroot/technology" package="org.eclipse.gmf/doc" dest="${checkoutDirectory}" />
	</target>
	<target name="all" depends="plugins,features,tests,examples,docs" unless="skipCheckout"/>
</project>