<?xml version="1.0" encoding="UTF-8"?>
<gmfgen:GenEditorGenerator
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgen="http://www.eclipse.org/gmf/2009/GenModel">
  <diagram
      visualID="1000"
      editPartClassName="DiagramEditPart"
      itemSemanticEditPolicyClassName="DiagramItemSemanticEditPolicy"
      canonicalEditPolicyClassName="DiagramCanonicalEditPolicy"
      iconProviderPriority="Low"
      validationProviderPriority="Low">
    <diagramRunTimeClass
        href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Diagram"/>
    <elementType
        xsi:type="gmfgen:MetamodelType"
        editHelperClassName="DiagramEditHelper"/>
    <viewmap
        xsi:type="gmfgen:ModeledViewmap">
      <figureModel
          href="pins.gmfgraph#pins"/>
    </viewmap>
    <domainDiagramElement
        href="pins.genmodel#//pins/Diagram"/>
    <childNodes
        visualID="3001"
        editPartClassName="PinsChildEditPart"
        itemSemanticEditPolicyClassName="PinsChildItemSemanticEditPolicy"
        canonicalEditPolicyClassName="PinsChildCanonicalEditPolicy"
        graphicalNodeEditPolicyClassName="PinsChildGraphicalNodeEditPolicy"
        createCommandClassName="PinsChildCreateCommand"
        containers="//@diagram/@topLevelNodes.0">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:MetamodelType"
          editHelperClassName="PinsChildEditHelper"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap"
          layoutType="FLOW_LAYOUT">
        <figureModel
            href="pins.gmfgraph#PinsChild"/>
      </viewmap>
      <modelFacet>
        <metaClass
            href="pins.genmodel#//pins/PinsChild"/>
        <containmentMetaFeature
            href="pins.genmodel#//pins/CustomPin/getChildren"/>
        <childMetaFeature
            href="pins.genmodel#//pins/CustomPin/getChildren"/>
      </modelFacet>
    </childNodes>
    <childNodes
        visualID="3002"
        editPartClassName="PinsChild2EditPart"
        itemSemanticEditPolicyClassName="PinsChild2ItemSemanticEditPolicy"
        canonicalEditPolicyClassName="PinsChild2CanonicalEditPolicy"
        graphicalNodeEditPolicyClassName="PinsChild2GraphicalNodeEditPolicy"
        createCommandClassName="PinsChild2CreateCommand"
        containers="//@diagram/@topLevelNodes.1">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:SpecializationType"
          metamodelType="//@diagram/@childNodes.0/@elementType"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap"
          layoutType="FLOW_LAYOUT">
        <figureModel
            href="pins.gmfgraph#PinsChild"/>
      </viewmap>
      <modelFacet>
        <metaClass
            href="pins.genmodel#//pins/PinsChild"/>
        <containmentMetaFeature
            href="pins.genmodel#//pins/VisiblePin/getChildren"/>
        <childMetaFeature
            href="pins.genmodel#//pins/VisiblePin/getChildren"/>
      </modelFacet>
    </childNodes>
    <childNodes
        visualID="3003"
        editPartClassName="PinsChild3EditPart"
        itemSemanticEditPolicyClassName="PinsChild3ItemSemanticEditPolicy"
        canonicalEditPolicyClassName="PinsChild3CanonicalEditPolicy"
        graphicalNodeEditPolicyClassName="PinsChild3GraphicalNodeEditPolicy"
        createCommandClassName="PinsChild3CreateCommand"
        containers="//@diagram/@topLevelNodes.2">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:SpecializationType"
          metamodelType="//@diagram/@childNodes.0/@elementType"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap"
          layoutType="FLOW_LAYOUT">
        <figureModel
            href="pins.gmfgraph#PinsChild"/>
      </viewmap>
      <modelFacet>
        <metaClass
            href="pins.genmodel#//pins/PinsChild"/>
        <containmentMetaFeature
            href="pins.genmodel#//pins/ColorPin/getChildren"/>
        <childMetaFeature
            href="pins.genmodel#//pins/ColorPin/getChildren"/>
      </modelFacet>
    </childNodes>
    <topLevelNodes
        visualID="2001"
        editPartClassName="CustomPinEditPart"
        itemSemanticEditPolicyClassName="CustomPinItemSemanticEditPolicy"
        canonicalEditPolicyClassName="CustomPinCanonicalEditPolicy"
        childNodes="//@diagram/@childNodes.0"
        graphicalNodeEditPolicyClassName="CustomPinGraphicalNodeEditPolicy"
        createCommandClassName="CustomPinCreateCommand">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:MetamodelType"
          editHelperClassName="CustomPinEditHelper"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap">
        <figureModel
            href="pins.gmfgraph#CustomPin"/>
      </viewmap>
      <modelFacet>
        <metaClass
            href="pins.genmodel#//pins/CustomPin"/>
        <containmentMetaFeature
            href="pins.genmodel#//pins/Diagram/getPins"/>
        <childMetaFeature
            href="pins.genmodel#//pins/Diagram/getPins"/>
      </modelFacet>
    </topLevelNodes>
    <topLevelNodes
        visualID="2002"
        editPartClassName="VisiblePinEditPart"
        itemSemanticEditPolicyClassName="VisiblePinItemSemanticEditPolicy"
        canonicalEditPolicyClassName="VisiblePinCanonicalEditPolicy"
        childNodes="//@diagram/@childNodes.1"
        graphicalNodeEditPolicyClassName="VisiblePinGraphicalNodeEditPolicy"
        createCommandClassName="VisiblePinCreateCommand">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:MetamodelType"
          editHelperClassName="VisiblePinEditHelper"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap"
          layoutType="TOOLBAR_LAYOUT">
        <figureModel
            href="pins.gmfgraph#VisiblePin"/>
      </viewmap>
      <behaviour
          xsi:type="gmfgen:GenVisualEffect"
          key="pins.diagram.edit.policies.VisiblePinPin2002Policy.KEY"
          editPolicyQualifiedClassName="pins.diagram.edit.policies.VisiblePinPin2002Policy"
          name="VisiblePinPin"
          pinKind="VisiblePin"
          operationName="getFigureVisiblePinWarningLabelFigure().setVisible"
          operationType="Boolean"
          oclExpression="self.getChildren->size()&lt;3"/>
      <modelFacet>
        <metaClass
            href="pins.genmodel#//pins/VisiblePin"/>
        <containmentMetaFeature
            href="pins.genmodel#//pins/Diagram/getPins"/>
        <childMetaFeature
            href="pins.genmodel#//pins/Diagram/getPins"/>
      </modelFacet>
      <labels
          visualID="5001"
          editPartClassName="VisiblePinWarningLabelEditPart"
          itemSemanticEditPolicyClassName="VisiblePinWarningLabelItemSemanticEditPolicy"
          elementIcon="true">
        <diagramRunTimeClass
            href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
        <viewmap
            xsi:type="gmfgen:ModeledViewmap">
          <figureModel
              href="pins.gmfgraph#VisiblePinWarningLabel"/>
        </viewmap>
        <modelFacet
            xsi:type="gmfgen:FeatureLabelModelFacet"
            parser="//@labelParsers/@implementations.0">
          <metaFeatures
              href="pins.genmodel#//pins/VisiblePin/WarningLabel"/>
        </modelFacet>
      </labels>
    </topLevelNodes>
    <topLevelNodes
        visualID="2003"
        editPartClassName="ColorPinEditPart"
        itemSemanticEditPolicyClassName="ColorPinItemSemanticEditPolicy"
        canonicalEditPolicyClassName="ColorPinCanonicalEditPolicy"
        childNodes="//@diagram/@childNodes.2"
        graphicalNodeEditPolicyClassName="ColorPinGraphicalNodeEditPolicy"
        createCommandClassName="ColorPinCreateCommand">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:MetamodelType"
          editHelperClassName="ColorPinEditHelper"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap"
          layoutType="TOOLBAR_LAYOUT">
        <figureModel
            href="pins.gmfgraph#ColorPin"/>
      </viewmap>
      <behaviour
          xsi:type="gmfgen:GenVisualEffect"
          key="pins.diagram.edit.policies.ColorPinPin2003Policy.KEY"
          editPolicyQualifiedClassName="pins.diagram.edit.policies.ColorPinPin2003Policy"
          name="ColorPinPin"
          pinKind="ColorPin"
          operationName="setBackgroundColor"
          operationType="Tuple(red:Integer, green:Integer, blue:Integer)"
          oclExpression="if self.getChildren->size() &lt; 4 then Tuple {red: Integer = 255, green: Integer = 128, blue: Integer = 0} else Tuple {red: Integer = 0, green: Integer = 128, blue: Integer = 0} endif"/>
      <modelFacet>
        <metaClass
            href="pins.genmodel#//pins/ColorPin"/>
        <containmentMetaFeature
            href="pins.genmodel#//pins/Diagram/getPins"/>
        <childMetaFeature
            href="pins.genmodel#//pins/Diagram/getPins"/>
      </modelFacet>
    </topLevelNodes>
    <palette>
      <groups
          title="pins">
        <entries
            xsi:type="gmfgen:ToolEntry"
            title="ColorPin"
            description="Create new ColorPin"
            genNodes="//@diagram/@topLevelNodes.2"/>
        <entries
            xsi:type="gmfgen:ToolEntry"
            title="CustomPin"
            description="Create new CustomPin"
            genNodes="//@diagram/@topLevelNodes.0"/>
        <entries
            xsi:type="gmfgen:ToolEntry"
            title="VisiblePin"
            description="Create new VisiblePin"
            genNodes="//@diagram/@topLevelNodes.1"/>
        <entries
            xsi:type="gmfgen:ToolEntry"
            title="PinsChild"
            description="Create new PinsChild"
            genNodes="//@diagram/@childNodes.0 //@diagram/@childNodes.1 //@diagram/@childNodes.2"/>
      </groups>
    </palette>
    <preferencePages
        xsi:type="gmfgen:GenStandardPreferencePage"
        iD="org.eclipse.gmf.tooling.examples.pins.diagram.general"
        name="Pins Diagram">
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.pins.diagram.appearance"
          name="Appearance"
          kind="Appearance"/>
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.pins.diagram.connections"
          name="Connections"
          kind="Connections"/>
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.pins.diagram.printing"
          name="Printing"
          kind="Printing"/>
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.pins.diagram.rulersAndGrid"
          name="Rulers And Grid"
          kind="RulersAndGrid"/>
    </preferencePages>
  </diagram>
  <plugin>
    <requiredPlugins>org.eclipse.gmf.tooling.runtime</requiredPlugins>
    <requiredPlugins>org.eclipse.ocl</requiredPlugins>
  </plugin>
  <editor/>
  <navigator>
    <childReferences
        child="//@diagram"/>
    <childReferences
        parent="//@diagram/@topLevelNodes.0"
        child="//@diagram/@childNodes.0"/>
    <childReferences
        parent="//@diagram"
        child="//@diagram/@topLevelNodes.0"/>
    <childReferences
        parent="//@diagram/@topLevelNodes.1"
        child="//@diagram/@childNodes.1"/>
    <childReferences
        parent="//@diagram"
        child="//@diagram/@topLevelNodes.1"/>
    <childReferences
        parent="//@diagram/@topLevelNodes.2"
        child="//@diagram/@childNodes.2"/>
    <childReferences
        parent="//@diagram"
        child="//@diagram/@topLevelNodes.2"/>
  </navigator>
  <diagramUpdater/>
  <propertySheet>
    <tabs
        xsi:type="gmfgen:GenStandardPropertyTab"
        iD="appearance"/>
    <tabs
        xsi:type="gmfgen:GenStandardPropertyTab"
        iD="diagram"/>
    <tabs
        xsi:type="gmfgen:GenCustomPropertyTab"
        iD="domain"
        label="Core">
      <filter
          xsi:type="gmfgen:TypeTabFilter">
        <types>org.eclipse.gmf.runtime.notation.View</types>
        <types>org.eclipse.gef.EditPart</types>
        <generatedTypes>abstractNavigatorItem</generatedTypes>
      </filter>
    </tabs>
  </propertySheet>
  <domainGenModel
      href="pins.genmodel#/"/>
  <labelParsers
      extensibleViaService="true">
    <implementations
        xsi:type="gmfgen:PredefinedParser"
        uses="//@diagram/@topLevelNodes.1/@labels.0/@modelFacet"/>
  </labelParsers>
  <contextMenus
      context="//@diagram">
    <items
        xsi:type="gmfgen:LoadResourceAction"/>
  </contextMenus>
</gmfgen:GenEditorGenerator>
