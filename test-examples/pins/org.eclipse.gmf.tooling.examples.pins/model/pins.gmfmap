<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="pins.ecore#//Diagram/getPins"/>
    <ownedChild>
      <domainMetaElement
          href="pins.ecore#//CustomPin"/>
      <tool
          xsi:type="gmftool:CreationTool"
          href="pins.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="pins.gmfgraph#CustomPin"/>
      <children>
        <containmentFeature
            href="pins.ecore#//CustomPin/getChildren"/>
        <ownedChild>
          <domainMetaElement
              href="pins.ecore#//PinsChild"/>
          <tool
              xsi:type="gmftool:CreationTool"
              href="pins.gmftool#//@palette/@tools.0/@tools.3"/>
          <diagramNode
              href="pins.gmfgraph#PinsChild"/>
        </ownedChild>
      </children>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="pins.ecore#//Diagram/getPins"/>
    <ownedChild>
      <domainMetaElement
          href="pins.ecore#//VisiblePin"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="pins.gmfgraph#VisiblePinWarningLabel"/>
        <features
            href="pins.ecore#//VisiblePin/WarningLabel"/>
      </labelMappings>
      <visualEffects
          oclExpression="self.getChildren->size()&lt;3">
        <diagramPin
            xsi:type="gmfgraph:VisiblePin"
            href="pins.gmfgraph#VisiblePinPin"/>
      </visualEffects>
      <tool
          xsi:type="gmftool:CreationTool"
          href="pins.gmftool#//@palette/@tools.0/@tools.2"/>
      <diagramNode
          href="pins.gmfgraph#VisiblePin"/>
      <children
          referencedChild="//@nodes.0/@ownedChild/@children.0/@ownedChild">
        <containmentFeature
            href="pins.ecore#//VisiblePin/getChildren"/>
      </children>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="pins.ecore#//Diagram/getPins"/>
    <ownedChild>
      <domainMetaElement
          href="pins.ecore#//ColorPin"/>
      <visualEffects
          oclExpression="if self.getChildren->size() &lt; 4 then Tuple {red: Integer = 255, green: Integer = 128, blue: Integer = 0} else Tuple {red: Integer = 0, green: Integer = 128, blue: Integer = 0} endif">
        <diagramPin
            xsi:type="gmfgraph:ColorPin"
            href="pins.gmfgraph#ColorPinPin"/>
      </visualEffects>
      <tool
          xsi:type="gmftool:CreationTool"
          href="pins.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="pins.gmfgraph#ColorPin"/>
      <children
          referencedChild="//@nodes.0/@ownedChild/@children.0/@ownedChild">
        <containmentFeature
            href="pins.ecore#//ColorPin/getChildren"/>
      </children>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="pins.ecore#//Diagram/getPins"/>
    <ownedChild>
      <domainMetaElement
          href="pins.ecore#//ChangeableColorPin"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="pins.gmfgraph#RedColorValueLabel"/>
        <features
            href="pins.ecore#//ChangeableColorPin/red"/>
        <editableFeatures
            href="pins.ecore#//ChangeableColorPin/red"/>
      </labelMappings>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="pins.gmfgraph#GreenColorValueLabel"/>
        <features
            href="pins.ecore#//ChangeableColorPin/green"/>
        <editableFeatures
            href="pins.ecore#//ChangeableColorPin/green"/>
      </labelMappings>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="pins.gmfgraph#BlueColorValueLabel"/>
        <features
            href="pins.ecore#//ChangeableColorPin/blue"/>
        <editableFeatures
            href="pins.ecore#//ChangeableColorPin/blue"/>
      </labelMappings>
      <visualEffects
          oclExpression="Tuple {red: Integer = self.red, green: Integer = self.green, blue: Integer = self.blue}">
        <diagramPin
            xsi:type="gmfgraph:ColorPin"
            href="pins.gmfgraph#PinChangeableColor"/>
      </visualEffects>
      <tool
          xsi:type="gmftool:CreationTool"
          href="pins.gmftool#//@palette/@tools.0/@tools.4"/>
      <diagramNode
          href="pins.gmfgraph#ChangeableColorPin"/>
    </ownedChild>
  </nodes>
  <diagram>
    <diagramCanvas
        href="pins.gmfgraph#pins"/>
    <domainModel
        href="pins.ecore#/"/>
    <domainMetaElement
        href="pins.ecore#//Diagram"/>
    <palette
        href="pins.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
