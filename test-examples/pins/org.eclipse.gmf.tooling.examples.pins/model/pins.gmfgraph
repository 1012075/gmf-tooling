<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition"
    name="pins">
  <figures
      name="Default">
    <descriptors
        name="ColorPinFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="ColorPinFigure"
          lineWidth="3"
          lineKind="LINE_DOT">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="lightBlue"/>
        <insets
            top="5"
            left="5"
            bottom="5"
            right="5"/>
        <pins
            xsi:type="gmfgraph:ColorPin"
            name="ColorPinPin"
            backgroundNotForeground="true"/>
      </actualFigure>
    </descriptors>
    <descriptors
        name="CustomPinFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="CustomPinFigure"/>
    </descriptors>
    <descriptors
        name="VisiblePinFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="VisiblePinFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"/>
        <children
            xsi:type="gmfgraph:Label"
            name="VisiblePinWarningLabelFigure"
            text="&lt;...>">
          <pins
              xsi:type="gmfgraph:VisiblePin"
              name="VisiblePinPin"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.2/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="PinsChildFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="PinChildLabel"
          text="Pin child">
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="darkBlue"/>
        <backgroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="darkBlue"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.3/@actualFigure"/>
    </descriptors>
    <descriptors
        name="ChangeableColorPinFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          lineWidth="6">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"
            majorSpacing="8"
            minorSpacing="8"/>
        <insets
            top="10"
            left="10"
            bottom="10"
            right="10"/>
        <pins
            xsi:type="gmfgraph:ColorPin"
            name="PinChangeableColor"/>
        <children
            xsi:type="gmfgraph:Label"
            name="RedLabelNameFigure"
            text="RED">
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="red"/>
        </children>
        <children
            xsi:type="gmfgraph:Label"
            name="RedColorValueFigure"
            text="0"/>
        <children
            xsi:type="gmfgraph:Label"
            name="GreenLabelNameFigure"
            text="GREEN">
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="green"/>
        </children>
        <children
            xsi:type="gmfgraph:Label"
            name="GreenColorValueFigure"
            text="0"/>
        <children
            xsi:type="gmfgraph:Label"
            name="BlueColorNameFigure"
            text="BLUE">
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="blue"/>
        </children>
        <children
            xsi:type="gmfgraph:Label"
            name="BlueColorValueFigure"
            text="0"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.5"/>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.3"/>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.1"/>
    </descriptors>
  </figures>
  <nodes
      name="ColorPin"
      figure="ColorPinFigure"/>
  <nodes
      name="CustomPin"
      figure="CustomPinFigure"/>
  <nodes
      name="VisiblePin"
      figure="VisiblePinFigure"/>
  <nodes
      name="PinsChild"
      figure="PinsChildFigure"/>
  <nodes
      name="ChangeableColorPin"
      figure="ChangeableColorPinFigure"/>
  <labels
      name="VisiblePinWarningLabel"
      figure="VisiblePinFigure"
      accessor="//@figures.0/@descriptors.2/@accessors.0"/>
  <labels
      name="PinsChildPinsChild"
      figure="PinsChildFigure"
      accessor="//@figures.0/@descriptors.3/@accessors.0"/>
  <labels
      name="RedColorValueLabel"
      figure="ChangeableColorPinFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.2"/>
  <labels
      name="GreenColorValueLabel"
      figure="ChangeableColorPinFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.1"/>
  <labels
      name="BlueColorValueLabel"
      figure="ChangeableColorPinFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.0"/>
</gmfgraph:Canvas>
