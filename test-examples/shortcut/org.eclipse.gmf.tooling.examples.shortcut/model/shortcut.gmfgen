<?xml version="1.0" encoding="UTF-8"?>
<gmfgen:GenEditorGenerator
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgen="http://www.eclipse.org/gmf/2009/GenModel">
  <diagram
      visualID="1000"
      editPartClassName="DiagramEditPart"
      itemSemanticEditPolicyClassName="DiagramItemSemanticEditPolicy"
      canonicalEditPolicyClassName="DiagramCanonicalEditPolicy"
      iconProviderPriority="Low"
      validationProviderPriority="Low">
    <diagramRunTimeClass
        href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Diagram"/>
    <elementType
        xsi:type="gmfgen:MetamodelType"
        editHelperClassName="DiagramEditHelper"/>
    <viewmap
        xsi:type="gmfgen:ModeledViewmap">
      <figureModel
          href="shortcut.gmfgraph#shortcut"/>
    </viewmap>
    <containsShortcutsTo>shortcut</containsShortcutsTo>
    <shortcutsProvidedFor>Shortcut</shortcutsProvidedFor>
    <domainDiagramElement
        href="shortcut.genmodel#//shortcut/Diagram"/>
    <topLevelNodes
        visualID="2001"
        editPartClassName="DiagramNodeEditPart"
        itemSemanticEditPolicyClassName="DiagramNodeItemSemanticEditPolicy"
        canonicalEditPolicyClassName="DiagramNodeCanonicalEditPolicy"
        graphicalNodeEditPolicyClassName="DiagramNodeGraphicalNodeEditPolicy"
        createCommandClassName="DiagramNodeCreateCommand">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
      <elementType
          xsi:type="gmfgen:MetamodelType"
          editHelperClassName="DiagramNodeEditHelper"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap"
          layoutType="FLOW_LAYOUT">
        <figureModel
            href="shortcut.gmfgraph#DiagramNode"/>
      </viewmap>
      <modelFacet>
        <metaClass
            href="shortcut.genmodel#//shortcut/DiagramNode"/>
        <containmentMetaFeature
            href="shortcut.genmodel#//shortcut/Diagram/nodes"/>
        <childMetaFeature
            href="shortcut.genmodel#//shortcut/Diagram/nodes"/>
      </modelFacet>
      <labels
          visualID="5001"
          editPartClassName="DiagramNodeNameEditPart"
          itemSemanticEditPolicyClassName="DiagramNodeNameItemSemanticEditPolicy"
          elementIcon="true">
        <diagramRunTimeClass
            href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Node"/>
        <viewmap
            xsi:type="gmfgen:ModeledViewmap">
          <figureModel
              href="shortcut.gmfgraph#DiagramNodeName"/>
        </viewmap>
        <modelFacet
            xsi:type="gmfgen:FeatureLabelModelFacet"
            parser="//@labelParsers/@implementations.0">
          <metaFeatures
              href="shortcut.genmodel#//shortcut/DiagramNode/name"/>
          <editableMetaFeatures
              href="shortcut.genmodel#//shortcut/DiagramNode/name"/>
        </modelFacet>
      </labels>
    </topLevelNodes>
    <links
        visualID="4001"
        editPartClassName="DiagramLinkEditPart"
        itemSemanticEditPolicyClassName="DiagramLinkItemSemanticEditPolicy"
        createCommandClassName="DiagramLinkCreateCommand"
        reorientCommandClassName="DiagramLinkReorientCommand">
      <diagramRunTimeClass
          href="../../../plugin/org.eclipse.gmf.runtime.notation/model/notation.genmodel#//notation/Edge"/>
      <elementType
          xsi:type="gmfgen:MetamodelType"
          editHelperClassName="DiagramLinkEditHelper"/>
      <viewmap
          xsi:type="gmfgen:ModeledViewmap">
        <figureModel
            href="shortcut.gmfgraph#DiagramLink"/>
      </viewmap>
      <modelFacet
          xsi:type="gmfgen:TypeLinkModelFacet">
        <metaClass
            href="shortcut.genmodel#//shortcut/DiagramLink"/>
        <containmentMetaFeature
            href="shortcut.genmodel#//shortcut/Diagram/links"/>
        <childMetaFeature
            href="shortcut.genmodel#//shortcut/Diagram/links"/>
        <sourceMetaFeature
            href="shortcut.genmodel#//shortcut/DiagramLink/sourceNode"/>
        <targetMetaFeature
            href="shortcut.genmodel#//shortcut/DiagramLink/targetNode"/>
      </modelFacet>
    </links>
    <palette>
      <groups
          title="shortcut">
        <entries
            xsi:type="gmfgen:ToolEntry"
            title="DiagramNode"
            description="Create new DiagramNode"
            genNodes="//@diagram/@topLevelNodes.0"/>
        <entries
            xsi:type="gmfgen:ToolEntry"
            title="DiagramLink"
            description="Create new DiagramLink"
            genLinks="//@diagram/@links.0"/>
      </groups>
    </palette>
    <preferencePages
        xsi:type="gmfgen:GenStandardPreferencePage"
        iD="org.eclipse.gmf.tooling.examples.shortcut.diagram.general"
        name="Shortcut Diagram">
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.shortcut.diagram.appearance"
          name="Appearance"
          kind="Appearance"/>
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.shortcut.diagram.connections"
          name="Connections"
          kind="Connections"/>
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.shortcut.diagram.printing"
          name="Printing"
          kind="Printing"/>
      <children
          xsi:type="gmfgen:GenStandardPreferencePage"
          iD="org.eclipse.gmf.tooling.examples.shortcut.diagram.rulersAndGrid"
          name="Rulers And Grid"
          kind="RulersAndGrid"/>
    </preferencePages>
  </diagram>
  <plugin>
    <requiredPlugins>org.eclipse.gmf.tooling.runtime</requiredPlugins>
  </plugin>
  <editor/>
  <navigator>
    <childReferences
        child="//@diagram"/>
    <childReferences
        parent="//@diagram"
        child="//@diagram/@topLevelNodes.0"/>
    <childReferences
        parent="//@diagram"
        child="//@diagram/@links.0"
        groupName="links"
        groupIcon="icons/linksNavigatorGroup.gif"/>
    <childReferences
        parent="//@diagram/@links.0"
        child="//@diagram/@topLevelNodes.0"
        referenceType="out_target"
        groupName="target"
        groupIcon="icons/linkTargetNavigatorGroup.gif"/>
    <childReferences
        parent="//@diagram/@topLevelNodes.0"
        child="//@diagram/@links.0"
        referenceType="in_source"
        groupName="incoming links"
        groupIcon="icons/incomingLinksNavigatorGroup.gif"/>
    <childReferences
        parent="//@diagram/@links.0"
        child="//@diagram/@topLevelNodes.0"
        referenceType="in_source"
        groupName="source"
        groupIcon="icons/linkSourceNavigatorGroup.gif"/>
    <childReferences
        parent="//@diagram/@topLevelNodes.0"
        child="//@diagram/@links.0"
        referenceType="out_target"
        groupName="outgoing links"
        groupIcon="icons/outgoingLinksNavigatorGroup.gif"/>
  </navigator>
  <diagramUpdater/>
  <propertySheet>
    <tabs
        xsi:type="gmfgen:GenStandardPropertyTab"
        iD="appearance"/>
    <tabs
        xsi:type="gmfgen:GenStandardPropertyTab"
        iD="diagram"/>
    <tabs
        xsi:type="gmfgen:GenCustomPropertyTab"
        iD="domain"
        label="Core">
      <filter
          xsi:type="gmfgen:TypeTabFilter">
        <types>org.eclipse.gmf.runtime.notation.View</types>
        <types>org.eclipse.gef.EditPart</types>
        <generatedTypes>abstractNavigatorItem</generatedTypes>
      </filter>
    </tabs>
  </propertySheet>
  <domainGenModel
      href="shortcut.genmodel#/"/>
  <labelParsers
      extensibleViaService="true">
    <implementations
        xsi:type="gmfgen:PredefinedParser"
        uses="//@diagram/@topLevelNodes.0/@labels.0/@modelFacet"/>
  </labelParsers>
  <contextMenus
      context="//@diagram">
    <items
        xsi:type="gmfgen:LoadResourceAction"/>
    <items
        xsi:type="gmfgen:CreateShortcutAction"/>
  </contextMenus>
</gmfgen:GenEditorGenerator>
