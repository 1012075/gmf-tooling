<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="shortcut.ecore#//Diagram/nodes"/>
    <ownedChild>
      <domainMetaElement
          href="shortcut.ecore#//DiagramNode"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="shortcut.gmfgraph#DiagramNodeName"/>
        <features
            href="shortcut.ecore#//DiagramNode/name"/>
        <editableFeatures
            href="shortcut.ecore#//DiagramNode/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="shortcut.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="shortcut.gmfgraph#DiagramNode"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="shortcut.ecore#//DiagramLink"/>
    <containmentFeature
        href="shortcut.ecore#//Diagram/links"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="shortcut.gmftool#//@palette/@tools.0/@tools.1"/>
    <diagramLink
        href="shortcut.gmfgraph#DiagramLink"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="shortcut.ecore#//DiagramLink/sourceNode"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="shortcut.ecore#//DiagramLink/targetNode"/>
  </links>
  <diagram>
    <diagramCanvas
        href="shortcut.gmfgraph#shortcut"/>
    <domainModel
        href="shortcut.ecore#/"/>
    <domainMetaElement
        href="shortcut.ecore#//Diagram"/>
    <palette
        href="shortcut.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
