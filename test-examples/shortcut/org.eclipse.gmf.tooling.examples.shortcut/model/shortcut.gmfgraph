<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition"
    name="shortcut">
  <figures
      name="Default">
    <descriptors
        name="DiagramNodeFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          lineWidth="2">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <insets
            top="3"
            left="3"
            bottom="3"
            right="3"/>
        <children
            xsi:type="gmfgraph:Label"
            name="DiagramNodeNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="DiagramLinkFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="DiagramLinkConnection"/>
      <accessors/>
    </descriptors>
  </figures>
  <nodes
      name="DiagramNode"
      figure="DiagramNodeFigure"/>
  <connections
      name="DiagramLink"
      figure="DiagramLinkFigure"/>
  <labels
      name="DiagramNodeName"
      figure="DiagramNodeFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
  <labels
      name="DiagramLinkName"
      figure="DiagramLinkFigure"
      accessor="//@figures.0/@descriptors.1/@accessors.0"/>
</gmfgraph:Canvas>
