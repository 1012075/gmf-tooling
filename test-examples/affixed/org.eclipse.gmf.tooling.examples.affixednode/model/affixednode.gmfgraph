<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition"
    name="affixednode">
  <figures
      name="Default">
    <descriptors
        name="IncomingCommunicatorFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="IncomingCommunicatorFigure"/>
    </descriptors>
    <descriptors
        name="OutcomingCommunicatorFigure">
      <actualFigure
          xsi:type="gmfgraph:Ellipse">
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="green"/>
      </actualFigure>
    </descriptors>
    <descriptors
        name="BundleFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="BundleFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="BUNDLE"
            text="BUNDLE"/>
        <children
            xsi:type="gmfgraph:Label"
            name="BundleNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.2/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="CommunicationFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="CommunicationFigure"
          targetDecoration="//@figures.0/@descriptors.4/@actualFigure"/>
    </descriptors>
    <descriptors
        name="CommunicationDecorator">
      <actualFigure
          xsi:type="gmfgraph:PolylineDecoration">
        <template
            x="-5"
            y="5"/>
        <template/>
        <template
            x="-5"
            y="-5"/>
      </actualFigure>
    </descriptors>
  </figures>
  <nodes
      name="IncomingCommunicator"
      figure="IncomingCommunicatorFigure"
      affixedParentSide="WEST"/>
  <nodes
      name="OutcomingCommunicator"
      figure="OutcomingCommunicatorFigure"
      affixedParentSide="EAST"/>
  <nodes
      name="Bundle"
      figure="BundleFigure"/>
  <connections
      name="Communication"
      figure="CommunicationFigure"/>
  <labels
      name="BundleName"
      figure="BundleFigure"
      accessor="//@figures.0/@descriptors.2/@accessors.0"/>
</gmfgraph:Canvas>
