<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="affixednode.ecore#//Canvas/bundles"/>
    <ownedChild>
      <domainMetaElement
          href="affixednode.ecore#//Bundle"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="affixednode.gmfgraph#BundleName"/>
        <features
            href="affixednode.ecore#//Bundle/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="affixednode.gmftool#//@palette/@tools.0/@tools.2"/>
      <diagramNode
          href="affixednode.gmfgraph#Bundle"/>
      <children>
        <containmentFeature
            href="affixednode.ecore#//Bundle/incomingCommunicator"/>
        <ownedChild>
          <domainMetaElement
              href="affixednode.ecore#//IncomingCommunicator"/>
          <tool
              xsi:type="gmftool:CreationTool"
              href="affixednode.gmftool#//@palette/@tools.0/@tools.0"/>
          <diagramNode
              href="affixednode.gmfgraph#IncomingCommunicator"/>
        </ownedChild>
      </children>
      <children>
        <containmentFeature
            href="affixednode.ecore#//Bundle/outcomingCommunicator"/>
        <ownedChild>
          <domainMetaElement
              href="affixednode.ecore#//OutcomingCommunicator"/>
          <tool
              xsi:type="gmftool:CreationTool"
              href="affixednode.gmftool#//@palette/@tools.0/@tools.1"/>
          <diagramNode
              href="affixednode.gmfgraph#OutcomingCommunicator"/>
        </ownedChild>
      </children>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="affixednode.ecore#//Communication"/>
    <containmentFeature
        href="affixednode.ecore#//Canvas/communications"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="affixednode.gmftool#//@palette/@tools.0/@tools.3"/>
    <diagramLink
        href="affixednode.gmfgraph#Communication"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="affixednode.ecore#//Communication/outcomingCommunicator"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="affixednode.ecore#//Communication/incomingCommunicator"/>
  </links>
  <diagram>
    <diagramCanvas
        href="affixednode.gmfgraph#affixednode"/>
    <domainModel
        href="affixednode.ecore#/"/>
    <domainMetaElement
        href="affixednode.ecore#//Canvas"/>
    <palette
        href="affixednode.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
