<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition"
    name="compartments">
  <figures
      name="Default">
    <descriptors
        name="NodeAFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="NodeAFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"/>
        <insets
            top="4"
            left="4"
            bottom="4"
            right="4"/>
        <children
            xsi:type="gmfgraph:Label"
            name="NodeANameFigure"
            text="NodeA"/>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="NodeCXYCompartmentFigure"/>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="NodeDListCompartmentFigure"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.1"/>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.2"/>
    </descriptors>
    <descriptors
        name="NodeBFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="NodeBFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"/>
        <insets
            top="4"
            left="4"
            bottom="4"
            right="4"/>
        <children
            xsi:type="gmfgraph:Label"
            name="NodeBNameFigure"
            text="NodeB"/>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="NodeEListCompFigure">
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="lightGreen"/>
        </children>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="NodeFListCompFigure">
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="lightBlue"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.1"/>
      <accessors
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.2"/>
    </descriptors>
    <descriptors
        name="NodeEFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"
            majorAlignment="CENTER"
            minorAlignment="CENTER"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="orange"/>
        <maximumSize/>
        <children
            xsi:type="gmfgraph:Label"
            name="NodeEFigure"
            text="NodeE">
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="darkGreen"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.2/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="NodeCFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"
            majorAlignment="CENTER"
            minorAlignment="CENTER"/>
        <children
            xsi:type="gmfgraph:Label"
            name="NodeCNameFigure"
            text="Process #"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.3/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="NodeGFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="ControlFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"
            vertical="true"
            matchMinorSize="true"
            forceSingleLine="true"/>
        <children
            xsi:type="gmfgraph:Label"
            name="NodeGNumber"
            text="0"/>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="AffixedChildrenCompartmentFigure"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="E_To_CLinkFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"/>
    </descriptors>
    <descriptors
        name="F_To_DLinkFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"/>
    </descriptors>
    <descriptors
        name="NodeFFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle">
        <layout
            xsi:type="gmfgraph:BorderLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="NodeFNameFigure"
            text="NodeF">
          <layoutData
              xsi:type="gmfgraph:BorderLayoutData"
              alignment="BEGINNING"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.7/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="NodeDFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="">
        <children
            xsi:type="gmfgraph:Label"
            name="NodeDNameFigure"
            text="NodeD"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.8/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="ChildOfAffixedNodeFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="ChildOfAffixedNode"
          text="ChildOfAffixedNode"/>
    </descriptors>
  </figures>
  <nodes
      name="NodeA"
      figure="NodeAFigure"/>
  <nodes
      name="NodeB"
      figure="NodeBFigure"/>
  <nodes
      name="ChildG"
      figure="NodeGFigure"
      affixedParentSide="SOUTH"/>
  <nodes
      name="ChildE"
      figure="NodeEFigure"/>
  <nodes
      name="ChildC"
      figure="NodeCFigure"/>
  <nodes
      name="ChildF"
      figure="NodeFFigure"/>
  <nodes
      name="NodeD"
      figure="NodeDFigure"/>
  <connections
      name="E_To_CLink"
      figure="E_To_CLinkFigure"/>
  <connections
      name="F_To_DLink"
      figure="F_To_DLinkFigure"/>
  <compartments
      name="NodeECompartment"
      figure="NodeEFigure"
      accessor="//@figures.0/@descriptors.1/@accessors.0"/>
  <compartments
      name="NodeFCompartment"
      figure="NodeBFigure"
      needsTitle="true"
      accessor="//@figures.0/@descriptors.1/@accessors.2"/>
  <compartments
      name="NodeCCompartment"
      figure="NodeAFigure"
      needsTitle="true"
      accessor="//@figures.0/@descriptors.0/@accessors.1"/>
  <compartments
      name="NodeDCompartment"
      figure="NodeAFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.2"/>
  <compartments
      name="AffixedChildrenCompartment"
      figure="NodeGFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.1"/>
  <labels
      name="NodeEName"
      figure="NodeEFigure"
      accessor="//@figures.0/@descriptors.2/@accessors.0"/>
  <labels
      name="NodeCName"
      figure="NodeCFigure"
      accessor="//@figures.0/@descriptors.3/@accessors.0"/>
  <labels
      name="AffixedNodeName"
      figure="NodeGFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.0"/>
  <labels
      name="NodeAName"
      figure="NodeAFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
  <labels
      name="NodeBName"
      figure="NodeBFigure"
      accessor="//@figures.0/@descriptors.1/@accessors.1"/>
  <labels
      name="NodeFName"
      figure="NodeFFigure"
      accessor="//@figures.0/@descriptors.7/@accessors.0"/>
  <labels
      name="NodeDLabel"
      figure="NodeDFigure"
      accessor="//@figures.0/@descriptors.8/@accessors.0"/>
  <labels
      name="ChildOfAffixedLabel"
      figure="ChildOfAffixedNodeFigure"/>
</gmfgraph:Canvas>
