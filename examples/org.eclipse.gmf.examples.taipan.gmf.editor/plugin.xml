   <extension point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent 
            id="org.eclipse.gmf.examples.taipan.gmf.editor.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanNavigatorContentProvider" 
            labelProvider="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanNavigatorLabelProvider"
            icon="../org.eclipse.gmf.examples.taipan.editor/icons/full/obj16/TaiPanModelFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="taipan_diagram"/>
               </and>
               <instanceof value="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="org.eclipse.gmf.examples.taipan.gmf.editor.isShortcut"/>
           	   </adapt>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="org.eclipse.gmf.examples.taipan.gmf.editor.isShortcut"/>
           	   </adapt>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.eclipse.gmf.examples.taipan.gmf.editor.navigatorSorter" 
               class="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="taipan_diagram"/>
                  </and>
                  <instanceof value="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <actionProvider
            id="org.eclipse.gmf.examples.taipan.gmf.editor.navigatorActionProvider"
            class="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="org.eclipse.gmf.examples.taipan.gmf.editor.navigator.TaiPanAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="org.eclipse.gmf.examples.taipan.gmf.editor.isShortcut"/>
           	   </adapt>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
