      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EClass_1001"
               eclass="EClass"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EClassNodeEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EPackage_1002"
               eclass="EPackage"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EPackageNodeEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EAnnotation_1003"
               eclass="EAnnotation"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EAnnotationNodeEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EDataType_1004"
               eclass="EDataType"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EDataTypeNodeEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EEnum_1005"
