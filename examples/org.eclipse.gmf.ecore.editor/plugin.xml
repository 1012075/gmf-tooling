         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <specializationType
               id="EAnnotationReferences_3001"
               edithelperadvice="org.eclipse.gmf.ecore.edit.helpers.EAnnotationReferencesLinkEditHelper">
            <specializes id="org.eclipse.gmf.runtime.emf.type.core.null"/>
            <param name="semanticHint" value="EAnnotationReferences_3001SemanticHint"/>
         </specializationType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EReference_3002"
