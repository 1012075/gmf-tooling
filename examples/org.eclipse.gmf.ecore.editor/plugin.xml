         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EPackage_2005"
               eclass="EPackage"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EPackageChildNodeEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EDataType_2006"
               eclass="EDataType"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EDataTypeChildNodeEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EEnum_2007"
