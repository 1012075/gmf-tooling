               edithelper="org.eclipse.gmf.ecore.edit.helpers.EReferenceLinkEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <metamodelType
               id="EReference_3003"
               eclass="EReference"
               edithelper="org.eclipse.gmf.ecore.edit.helpers.EReferenceLinkEditHelper">
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore">
         <specializationType
               id="EClassESuperTypes_3004"
               edithelperadvice="org.eclipse.gmf.ecore.edit.helpers.EClassESuperTypesLinkEditHelper">
            <specializes id="org.eclipse.gmf.runtime.emf.type.core.null"/>
            <param name="semanticHint" value="EClassESuperTypes_3004SemanticHint"/>
         </specializationType>
      </metamodel>
   </extension>

</plugin>
