«DEFINE emptyIf FOR ecore::EPackage-»
«IF eClassifiers->size() > 0»
«ENDIF»
«ENDDEFINE»

«DEFINE ifWithBody FOR ecore::EPackage-»
«IF true»
	Start
		«'prefix1'.concat(eClassifiers->size().repr())»
	Other
		«'prefix2'.concat(eClassifiers->size().repr())»
	End
«ENDIF»
«ENDDEFINE»

«DEFINE ifWithElse FOR ecore::EPackage-»
«IF false»
«ELSE»
	Start
		«'prefix1'.concat(eClassifiers->size().repr())»
	Other
		«'prefix2'.concat(eClassifiers->size().repr())»
	End
«ENDIF»
«ENDDEFINE»

«DEFINE ifWithElseIfs FOR ecore::EPackage-»
«IF eClassifiers->size() = 1»
	«'1: '.concat(eClassifiers->size().repr())»
«ELSEIF eClassifiers->size() = 2»
	«'2: '.concat(eClassifiers->size().repr())»
«ELSEIF eClassifiers->size() = 3»
	«'3: '.concat(eClassifiers->size().repr())»
«ELSEIF eClassifiers->size() = 4»
	«'4: '.concat(eClassifiers->size().repr())»
«ELSEIF eClassifiers->size() = 5»
	«'5: '.concat(eClassifiers->size().repr())»
«ELSE»
	«'unlimited: '.concat(eClassifiers->size().repr())»
«ENDIF»
«ENDDEFINE»
