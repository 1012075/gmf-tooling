modeltype ecore uses "http://www.eclipse.org/emf/2002/Ecore";

library TypeSelect;

helper ecore::EPackage::typeSelectCall() : Sequence(ecore::EClass) {
	return (self.eClassifiers)->select(it | it.oclIsKindOf(ecore::EClass))->collect(it | it.oclAsType(ecore::EClass))->asOrderedSet()->asSequence()
}

helper Sequence(ecore::EClassifier)::typeSelectOnList() : Sequence(ecore::EClass) {
	return (self)->select(it | it.oclIsKindOf(ecore::EClass))->collect(it | it.oclAsType(ecore::EClass))
}

helper Set(ecore::EClassifier)::typeSelectOnSet() : Sequence(ecore::EClass) {
	return (self)->select(it | it.oclIsKindOf(ecore::EClass))->collect(it | it.oclAsType(ecore::EClass))->asSet()->asSequence()
}

helper Collection(ecore::EClassifier)::typeSelectOnCollection() : Sequence(ecore::EClass) {
	return self->collect(it | it.oclAsType(ecore::EClass))->asSequence()
}