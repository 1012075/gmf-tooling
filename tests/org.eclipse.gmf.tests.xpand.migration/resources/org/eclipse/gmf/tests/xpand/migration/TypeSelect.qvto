modeltype ecore uses "http://www.eclipse.org/emf/2002/Ecore";

library TypeSelect;

helper typeSelectCall(ePackage : ecore::EPackage) : Sequence(ecore::EClass) {
	return ePackage.eClassifiers->select(element | element.oclIsKindOf(ecore::EClass))->collect(element | element.oclAsType(ecore::EClass))
}
