modeltype ecore uses "http://www.eclipse.org/emf/2002/Ecore";

library ChainExpression;

helper ecore::EClass::chainExpression(eFeature : ecore::EStructuralFeature) : String {
	self.eUnset(eFeature); return self.name
}

helper ecore::EClass::nestedChainExpressions(eFeature1 : ecore::EStructuralFeature, eFeature2 : ecore::EStructuralFeature) : String {
	self.eUnset(eFeature1); self.eUnset(eFeature2); return self.name
}