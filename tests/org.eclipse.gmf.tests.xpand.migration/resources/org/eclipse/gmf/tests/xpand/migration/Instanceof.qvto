modeltype ecore uses "http://www.eclipse.org/emf/2002/Ecore";

library Instanceof;

helper instanceofSize(eObject : ecore::EObject) : Boolean {
	return eObject.oclIsKindOf(ecore::EClass)
}

helper notInstanceofSize(eObject : ecore::EObject) : Boolean {
	return not eObject.oclIsKindOf(ecore::EClass)
}

helper notInstanceofSize1(eObject : ecore::EObject) : Boolean {
	return not eObject.oclIsKindOf(ecore::EClass)
}

helper instanceofIsEmpty(eObject : ecore::EObject) : Boolean {
	return not eObject.oclIsKindOf(ecore::EClass)
}

helper notInstanceofIsEmpty(eObject : ecore::EObject) : Boolean {
	return eObject.oclIsKindOf(ecore::EClass)
}

helper instanceofIsEmptyFalse(eObject : ecore::EObject) : Boolean {
	return eObject.oclIsKindOf(ecore::EClass)
}