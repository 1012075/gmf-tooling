import library XpandOclAnyOperations;

library OperationCall_BooleanOperations;

helper Boolean::booleanNegation() : Boolean {
	return not self
}

helper Boolean::booleanCompareTo(another : OclAny) : Boolean {
	return self.xpandCompareTo(another)
}

helper Boolean::booleanToString() : String {
	return self.repr()
}

helper Boolean::booleanEQ(another : OclAny) : Boolean {
	return self = another
}

helper Boolean::booleanNotEQ(another : OclAny) : Boolean {
	return self <> another
}