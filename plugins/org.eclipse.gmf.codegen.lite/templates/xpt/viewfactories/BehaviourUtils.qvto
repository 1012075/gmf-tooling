modeltype genModel uses "http://www.eclipse.org/gmf/2009/GenModel";

library BehaviourUtils;

helper openDiagramBehaviours(e : genModel::GenCommonBase) : Sequence(genModel::OpenDiagramBehaviour) {
	return (e.behaviour)[genModel::OpenDiagramBehaviour]->union(((e.behaviour)[genModel::SharedBehaviour].delegate)[genModel::OpenDiagramBehaviour]->asSet())->asSequence()
}
